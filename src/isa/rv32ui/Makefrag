#=======================================================================
# Makefrag for rv32ui tests
#-----------------------------------------------------------------------

rv32ui_test = \
	simple \
	add addi \
	and andi \
	auipc \
	beq bge bgeu blt bltu bne \
	fence_i \
	jal jalr \
	lb lbu lh lhu lw \
	lui \
	or ori \
	sb sh sw \
	sll slli \
	slt slti sltiu sltu \
	sra srai \
	srl srli \
	sub \
	xor xori \

rv32ui_tests = $(addprefix rv32ui-, $(rv32ui_test))
